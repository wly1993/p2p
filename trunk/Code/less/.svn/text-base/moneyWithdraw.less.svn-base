@charset "UTF-8";
//引入字体样式库
@import "../lib/font-awesome/css/font-awesome.min.css";
//定义颜色
@basecolor: #2e8ece;
//导航栏
#bs-example-navbar-collapse-2{
    padding-top: 20px;
    span{
        font-size: 23px;
        color: @basecolor;
  }
}
//正文
#contant{
  .topbox{
    height:300px;
    background-color:darken(#c7ddef,1);
    padding-left: 20px;
    border-left: 2px solid @basecolor;
    .firstP{
      font-size: 25px;
      color: #00b3ee;
      padding-top: 20px;
    }
  }
  .row{
    height: 50px;
    font-size: 18px;
    margin-bottom: 50px;
    div{
      line-height: 50px;
      span{
        color: @basecolor;
      }
    }
  }
  .form-horizontal{
    width:500px;
    margin-left: 60px;
    .dropdown{
      max-width: 100%;
      .btn-default{
        width: 100%;
        margin-left: 0px;
      }
    }
    button{
      margin-left: 150px;
    }

  }
}